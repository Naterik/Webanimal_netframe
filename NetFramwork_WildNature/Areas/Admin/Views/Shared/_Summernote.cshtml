@model NetFramwork_WildNature.Dao.Summernote

@if (Model.loadLybrary)
{
    <link href="~/Content/Summerfile/summernote-bs4.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    <script src="~/Content/Summerfile/summernote-bs4.min.js"></script>
}

<script>
    $(document).ready(function () {
        $('#@Model.Ideditor').summernote({
            height: @Model.height,
            toolbar: '@Html.Raw(Model.toolbar)'
        });
    });
</script>
